
@{
    ViewBag.Title = "Result";
}

<section>
    <!-- Page content-->
    <div class="content-wrapper">
        <h3>
            La probabilidad de que el usuario deje de frecuentar en la tienda es de:
            <small></small>
        </h3>
        <!-- START panel-->
        <div class="panel panel-default">
            <div class="panel-heading"></div>
           

            <div class="progress">
                <div class="progress-bar" role="progressbar" style="width: @ViewBag.Result%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">@ViewBag.Result %</div>
            </div>

        </div>
    </div>
</section>

